#include <stdio.h>

int multiply(int a, int b);
int multiplyadd(int a, int b, int c);

int main(void)
{
	int a, b, c, d;
	a = 2;
	b = 3;
	c = 4;
	d = multiply(a,b);
	printf("a * b is %d\n", d);
	d = multiplyadd(a,b,c);
	printf("a * b + c is %d\n", d);

	return 0;
}

int multiply(int a, int b)
{
	return (a*b);
}

int multiplyadd(int a, int b, int c)
{
	return ((a*b)+c);
}
